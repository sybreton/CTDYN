      real function c2m2 (n)
        integer n
        real cgret
       REAL x_in, bct, c3,mmm
       COMMON/ppar/x_in,bct,c3,mmm

c        cgret = n * (n + 0.1E1) / (0.2E1 * n - 0.1E1) / (0.2E1 * n + 0.1E
c     #1)
        cgret  = (1.0+n)*(-1.0+abs(mmm)+n)*(abs(mmm)+n)/(1.0-n-4.0*n**2+4.0*n**3)
        c2m2 = cgret
