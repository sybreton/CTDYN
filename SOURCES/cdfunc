      real function d10 (n)
        integer n
        real cgret

      REAL x_in, bct, c3,mmm
      COMMON/ppar/x_in,bct,c3,mmm

        cgret = -0.3E1 * (n + 0.2E1) * (n - 0.1E1) * (n*(n+1)-3*mmm**2)/ (0.2E1 * n - 0.1E1) 
     #/ (0.2E1 * n + 0.3E1)/n/(n+1.0)

        d10 = cgret
        return
      end
      real function d3p4 (n)
