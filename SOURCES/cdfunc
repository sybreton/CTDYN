      real function c2p2 (n)
        integer n
        real cgret
       REAL x_in, bct, c3,mmm
       COMMON/ppar/x_in,bct,c3,mmm

c        cgret = n * (n + 0.1E1) / (0.2E1 * n + 0.1E1) / (0.2E1 * n + 0.3E
