      real function c20 (n)
        integer n
        real cgret

       REAL x_in, bct, c3,mmm
       COMMON/ppar/x_in,bct,c3,mmm

        cgret = ((n*(n+1)-3*mmm**2)*2*(n*(n+1.)-3)/n/(n+1.)/(2*n-1.)/(2*n+3.) +1)/3.
        cgret = (0.2E1 * n ** 2 + 0.2E1 * n - 0.3E1) / (0.2E1 * n - 0.1E1
     #) / (0.2E1 * n + 0.3E1)
        c20 = cgret

        return
      end

      real function c3p3 (n)
